{
  "id": "/sensu",
  "cmd": null,
  "cpus": 0.1,
  "mem": 256,
  "disk": 0,
  "instances": 1,
  "constraints": [
    [
      "rack",
      "LIKE",
      "mesos-slave-1"
    ]
  ],
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/etc/sensu/check.d",
        "hostPath": "/mnt/docker/sensu/checks",
        "mode": "RW"
      },
      {
        "containerPath": "/etc/sensu/handlers",
        "hostPath": "/mnt/docker/sensu/handlers",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "sstarcher/sensu",
      "network": "BRIDGE",
      "portMappings": [],
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "env": {
    "REDIS_HOST": "5s.lab.int",
    "REDIS_AUTO_RECONNECT": "true",
    "RUNTIME_INSTALL": "graphite",
    "REDIS_RECONNECT_ON_ERROR": "false",
    "TRANSPORT_NAME": "redis",
    "API_HOST": "5s.lab.int"
  },
  "labels": {
    "tag": "monitoring"
  },
  "portDefinitions": [
    {
      "port": 10002,
      "protocol": "tcp",
      "name": "default",
      "labels": {}
    }
  ],
  "args": [
    "server"
  ]
}